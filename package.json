{
  "name": "train-track-simulation",
  "type": "module",
  "version": "0.0.1",
  "description": "A train track simulation library",
  "author": "lingann",
  "browser": "dist/index.umd.js",
  "license": "ISC",
  "homepage": "https://github.com/Lingann/train-track-simulation#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lingann/train-track-simulation.git"
  },
  "bugs": {
    "url": "https://github.com/Lingann/train-track-simulation/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "patch-package",
    "test": "vitest -u",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "dev": "vite",
    "build": "vite build"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "dependencies": {
    "gsap": "^3.12.5",
    "three": "^0.164.1",
    "vue": "3.3.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.0",
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@types/node": "^18.19.21",
    "@types/three": "^0.164.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.56.0",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "inquirer": "^9.1.4",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "sass": "1.56.2",
    "semver": "^7.3.8",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^3.0.0",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^1.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,html}": [
      "eslint",
      "stylelint"
    ],
    "*.{scss,less,style}": [
      "stylelint"
    ]
  }
}
